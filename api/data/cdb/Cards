#
# Template for card data
# -----------------------
# <VENDOR NAME>:<DEVICE NAME>  {
#  Driver               = driver...
#  Memory               = [ value or Auto ]
#  DacChip              = RamDAC chip
#  GraphicsChip         = Graphics Chip
#  ClockChip            = Clock Chips
#  Clocks               = clock list like 10,20,30
#  Option               = option like noaccel,nolinear
#  RawData              = full formatted section line like:
#                         ( Option "nolinear",Option "IOBase" "0x1020" )
# }

#
# Chips & Technologies CT65520
#
CHIPS & TECHNOLOGIES:CT65520   {
 GraphicsChip = ct65520
 Driver       = chips
 Option       = suspend_hack,STN,no_stretch,no_center,use_modeline,fix_panel_size
}

#
# Chips & Technologies CT65525
#
CHIPS & TECHNOLOGIES:CT65525   {
 GraphicsChip = ct65525
 RawData      = Option "MemBase" "0x03b00000"
 Option       = nolinear
 Driver       = chips
}

#
# Chips & Technologies CT65530
#
CHIPS & TECHNOLOGIES:CT65530   {
 GraphicsChip = ct65530
 Driver       = chips
}

#
# Chips & Technologies CT65535
#
CHIPS & TECHNOLOGIES:CT65535   {
 GraphicsChip = ct65535
 RawData      = Option "Textclockfreq" "25.175"
 Option       = hw_clocks
 Driver       = chips
}

#
# Chips & Technologies CT65540
#
CHIPS & TECHNOLOGIES:CT65540   {
 GraphicsChip = ct65540
 Option       = use_18bit_bus
 Driver       = chips
}

